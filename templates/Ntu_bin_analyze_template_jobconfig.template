universe = vanilla
executable = JOBDIR/SCRIPTFILE
arguments = EVENTSPERJOB $(Process)
should_transfer_files = YES
when_to_transfer_output = ON_EXIT_OR_EVICT
getenv = True
transfer_input_files = LIBFILE,VERBNAME,CFGFILE,TREELISTFILE
transfer_output_files = OUTFILES
log = out/VERBNAME_$(Cluster)_$(Process).log
output = out/VERBNAME_$(Cluster)_$(Process).stdout
error = out/VERBNAME_$(Cluster)_$(Process).stderr
Queue NJOBS
